@model IEnumerable<IICURas.Models.CompletedTrainingListViewModel>

@{
    ViewBag.Title = "List of Completed Training";
}

<div class="col-xs-12 table-response">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(m => m.PubNumber)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.Points)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.StartTime)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.CompletionTime)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.TimeUsed)
            </th>
            <th>  @Html.DisplayNameFor(m => m.Status)              
            </th>
            <th></th>
        </tr>
        
        @Html.EditorForModel()
    </table>
</div>

<div class=" row col-xs-12">
    <br />
    <br />
    <br />
    <a class="btn btn-default btn-lg" onclick="window.history.back()">Back</a>
</div>
